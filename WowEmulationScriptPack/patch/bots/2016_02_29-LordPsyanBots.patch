+
+enum GuildTaskType
+{
+    GUILD_TASK_TYPE_NONE = 0,
+    GUILD_TASK_TYPE_ITEM = 1,
+    GUILD_TASK_TYPE_KILL = 2
+};
+
+GuildTaskMgr::GuildTaskMgr()
+{
+}
+
