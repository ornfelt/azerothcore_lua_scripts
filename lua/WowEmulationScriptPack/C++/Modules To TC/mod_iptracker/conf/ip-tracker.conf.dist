[worldserver]

#
#    IpTracker.Enabled
#        Description: Enable the mod-ip-tracker
#        Default:     0 - Disabled
#                     1 - Enabled

IpTracker.Enabled = 0

#
#    IpTracker.CleanOlderThanDays
#        Description: Clean old logs at server startup
#        Default:       0  - Disabled (do not remove anything)
#                       90 - Remove ip logs older than 90 days

IpTracker.CleanOlderThanDays = 0
