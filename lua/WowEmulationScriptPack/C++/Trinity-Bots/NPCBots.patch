+                Rand() < (50 + 85 * me->GetMap()->IsDungeon()) && GetHealthPCT(opponent) < 25 &&
+                opponent->GetHealth() > me->GetMaxHealth() / 2 && dist < CalcSpellMaxRange(DRAIN_SOUL_1))
