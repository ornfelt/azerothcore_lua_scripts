+                            break;
+                    }
+                    if (fearTarget && tCount > 1 && doCast(fearTarget, GetSpell(INTIMIDATING_SHOUT_1)))
+                        return;
+                }
+                //Defend myself
+                if (b_attackers.size() > 1 && (!IsTank() || GetHealthPCT(me) < 50))
