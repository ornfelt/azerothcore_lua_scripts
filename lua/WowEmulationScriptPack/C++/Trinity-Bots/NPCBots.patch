+BotDataDumpResult NPCBotsDump::Load(std::string const& file)
+{
+    std::ifstream input(file.c_str());
+    if (!input)
+        return BOT_DUMP_FAIL_FILE_NOT_EXIST;
+
+    return LoadDump(input);
+}
