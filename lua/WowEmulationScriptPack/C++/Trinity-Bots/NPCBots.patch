+#include <string>
+
+enum BotDataDumpResult
+{
+    //all
+    BOT_DUMP_SUCCESS                    = 0,
+    //write
+    BOT_DUMP_FAIL_FILE_ALREADY_EXISTS,
+    BOT_DUMP_FAIL_CANT_WRITE_TO_FILE,
+    BOT_DUMP_FAIL_INCOMPLETE,
+    //load
