+                pctbonus += 0.12f;
+            //Improved Death Strike (part 1): 30% bonus damage for Death Strike
+            if ((_spec == BOT_SPEC_DK_BLOOD) && lvl >= 62 && baseId == DEATH_STRIKE_1)
+                pctbonus += 0.3f;
