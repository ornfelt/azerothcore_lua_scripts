+        void CheckPolymorph(uint32 diff)
+        {
+            if (poly == false && IsSpellReady(POLYMORPH_1, diff) && !IsCasting())
+            {
+                if (Unit* target = FindPolyTarget(CalcSpellMaxRange(POLYMORPH_1)))
+                {
+                    if (doCast(target, GetSpell(POLYMORPH_1)))
+                        return;
