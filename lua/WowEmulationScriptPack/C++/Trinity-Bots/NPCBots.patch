+    if (!me->IsValidAttackTarget(attacker) || !attacker->isTargetableForAttack() || IsInBotParty(attacker))
+        return;
+
+    SetBotCommandState(BOT_COMMAND_COMBATRESET);
+    me->Attack(attacker, IsPetMelee());
