+                return;
+            }
+
+            if (IsSpellReady(BLOCKING_1, diff) && !me->getAttackers().empty() && Rand() < 40)
+            {
+                me->CastSpell(me, GetSpell(BLOCKING_1), true);
+                SetSpellCooldown(BLOCKING_1, 20000);
+                return;
+            }
+        }
