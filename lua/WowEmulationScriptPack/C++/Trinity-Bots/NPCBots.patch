+            //enchant_amount = pEnchant->EffectPointsMin[s];
+            enchant_spell_id = pEnchant->EffectArg[s];
+
+            switch (enchant_display_type)
+            {
+                case ITEM_ENCHANTMENT_TYPE_EQUIP_SPELL:
+                {
+                    if (!enchant_spell_id)
+                        break;
+                    int32 basepoints = 0;
