+//%health unit is going to have after x ms
+//0-100
+uint8 bot_ai::GetExpectedHPPCT(Unit const* u, uint32 mseconds) const
+{
+    if (IAmFree())
+        return GetHealthPCT(u);
+
+    int32 pct = int32(GetHealthPCT(u)) + int32(GetHPPCTPS(u) * (mseconds * 0.001f));
+
+    if (pct > 100)
+        pct = 100;
+    else if (pct < 0)
+        pct = 0;
+
+    return uint8(pct);
+}
