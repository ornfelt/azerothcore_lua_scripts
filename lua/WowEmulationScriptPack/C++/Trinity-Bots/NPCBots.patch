+
+            if (FindAffectedTarget(FEAR, me->GetGUID(), 70, 255))
+                return;
+
+            Unit* feartarget = FindFearTarget(CalcSpellMaxRange(FEAR_1));
+            if (feartarget && doCast(feartarget, FEAR))
+                return;
+        }
