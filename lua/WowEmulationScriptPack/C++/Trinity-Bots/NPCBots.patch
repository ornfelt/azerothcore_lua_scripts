+            end_pos = pos;
+            //TC_LOG_ERROR("scripts", "import: ReGuidCreature end_pos %u", uint32(end_pos));
+        }
+
+        if (begin_pos && end_pos)
+        {
+            uint32 guidVal;
+            StringToVal(line, guidVal, begin_pos, end_pos);
