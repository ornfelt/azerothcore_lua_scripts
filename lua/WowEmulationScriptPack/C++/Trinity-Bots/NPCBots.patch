+            }
+        }
+        else if (!IsCasting(mover) && (!IsShootingWand(mover) || Rand() < 10))
+        {
+            _calculatePos(pos);
+            float maxdist = std::max<float>(master->GetBotMgr()->GetBotFollowDist() * (master->isMoving() ? 0.02f : 0.35f), 4.f);
+            if (!me->isMoving())
