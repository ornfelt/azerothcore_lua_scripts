+                return;
+
+            banishTimer = 1600;
+
+            uint32 BANISH = GetSpell(BANISH_1);
+            if (!BANISH)
+                return;
+
+            if (FindAffectedTarget(BANISH, me->GetGUID(), 70, 255))
