+            u = ObjectAccessor::GetUnit(*me, spell->m_targets.GetObjectTargetGUID());
+            if (u && !u->isMoving() && me->GetDistance(u) < 4 && IsSpellReady(ANTI_MAGIC_ZONE_1, diff, false) &&
