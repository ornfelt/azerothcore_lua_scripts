+                        ++count;
+                        if (cre->IsAlive())
+                            ++alivecount;
+                    }
+                }
+            }
+            if (count == 0)
+                continue;
+
+            char const* bclass;
+            switch (i)
+            {
+                case BOT_CLASS_WARRIOR:         bclass = "Warriors";        break;
