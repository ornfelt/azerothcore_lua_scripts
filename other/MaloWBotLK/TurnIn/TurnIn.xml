	<Button name="TI_PriorityList" virtual="true" hidden="false">
		<Size>
			<AbsDimension x="235" y="20" />
		</Size>
		<Scripts><!--
			<OnEnter>
				getglobal(self:GetName() .. "_Background"):SetVertexColor(1,1,1,0.4);
			</OnEnter>
			<OnLeave>
				getglobal(self:GetName() .. "_Background"):SetVertexColor(0,0,0,0.4);
			</OnLeave>
